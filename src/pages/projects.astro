---
import BaseLayout from '../layouts/BaseLayout.astro';
import ProjectsBackgroundAnimation from '../components/ProjectsBackgroundAnimation.astro';

const title = 'AI Startup Portfolio - Fintech & AI Companies | WiseVenture';
const description = 'Explore our innovative AI startup and fintech investment portfolio. We invest in AI native companies, fintech startups, and provide tech advisory services to help them grow.';
const keywords = 'AI startup portfolio, fintech companies, AI native startups, startup investments, AI companies, fintech investments, portfolio companies, AI technology startups';
---

<BaseLayout title={title} description={description} keywords={keywords}>
  <div class="min-h-screen py-8 sm:py-12 bg-black relative">
    <ProjectsBackgroundAnimation client:only />
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 relative z-10">
      <!-- Header -->
      <div class="text-center mb-10 sm:mb-16">
        <div class="inline-block mb-3 sm:mb-4">
          <span class="text-white text-xl sm:text-2xl md:text-3xl lg:text-4xl font-bold">OUR PORTFOLIO</span>
        </div>
        <h1 class="text-xl sm:text-2xl md:text-3xl lg:text-4xl font-bold mb-3 sm:mb-4 px-2">
          <span class="text-white">Projects</span>
        </h1>
        <p class="text-base sm:text-lg md:text-xl text-white/60 max-w-3xl mx-auto px-4 font-light">
          Explore our innovative AI startup and fintech investments, witnessing how AI technology transforms the business world
        </p>
      </div>

      <!-- Projects Grid -->
      <div class="grid md:grid-cols-2 gap-6 sm:gap-8">
        <!-- Minerva Electronics -->
        <div class="bg-black border border-white/15 rounded-none transition-all duration-300 overflow-hidden hover:border-white/25">
          <div class="p-5 sm:p-6 md:p-8">
            <div class="flex items-center justify-between mb-4">
              <span class="px-3 py-1 rounded-none text-xs font-normal uppercase tracking-wider bg-gray-300 text-black border border-gray-400">
                Smart Retail AI
              </span>
            </div>
            <a
              href="https://www.minervaelectronics.com/"
              target="_blank"
              rel="noopener noreferrer"
              class="text-2xl sm:text-3xl font-bold mb-3 sm:mb-4 block hover:opacity-80 transition-opacity text-yellow-200"
            >
              Minerva Electronics
            </a>
            <p class="text-white/60 mb-4 sm:mb-6 leading-relaxed text-sm sm:text-base font-light">
              Smart retail solutions provider leveraging AI technology to optimize customer experience and operational efficiency in retail.
            </p>
            
            <div class="mb-4 sm:mb-6 space-y-3 sm:space-y-4">
              <div>
                <h3 class="text-base sm:text-lg font-semibold text-white mb-2 flex items-center">
                  <span class="w-2 h-2 rounded-full bg-white/40 mr-2"></span>
                  Technology Stack
                </h3>
                <p class="text-white/50 text-xs sm:text-sm font-light">AI-driven smart retail systems, data analytics platform, customer behavior prediction</p>
              </div>
              <div>
                <h3 class="text-base sm:text-lg font-semibold text-white mb-2 flex items-center">
                  <span class="w-2 h-2 rounded-full bg-white/40 mr-2"></span>
                  AI Application Highlights
                </h3>
                <p class="text-white/60 leading-relaxed text-sm sm:text-base font-light">
                  Through machine learning algorithms analyzing customer purchase behavior, providing personalized recommendations and inventory optimization suggestions to enhance retail operational efficiency and customer satisfaction.
                </p>
              </div>
            </div>

            <a
              href="https://www.minervaelectronics.com/"
              target="_blank"
              rel="noopener noreferrer"
              class="inline-flex items-center text-base sm:text-lg md:text-xl font-normal transition-all underline underline-offset-4 decoration-yellow-200/30 hover:decoration-yellow-200/50 text-yellow-200 hover:text-yellow-100"
            >
              Learn More
              <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14" />
              </svg>
            </a>
          </div>
        </div>

        <!-- WooTogether -->
        <div class="bg-black border border-white/15 rounded-none transition-all duration-300 overflow-hidden hover:border-white/25">
          <div class="p-5 sm:p-6 md:p-8">
            <div class="flex items-center justify-between mb-4">
              <span class="px-3 py-1 rounded-none text-xs font-normal uppercase tracking-wider bg-gray-300 text-black border border-gray-400">
                Social Platform AI
              </span>
            </div>
            <a
              href="https://wootogether.com"
              target="_blank"
              rel="noopener noreferrer"
              class="text-2xl sm:text-3xl font-bold mb-3 sm:mb-4 block hover:opacity-80 transition-opacity text-yellow-200"
            >
              WooTogether
            </a>
            <p class="text-white/60 mb-4 sm:mb-6 leading-relaxed text-sm sm:text-base font-light">
              Innovative social platform using AI technology to enhance user interaction and community connections.
            </p>
            
            <div class="mb-4 sm:mb-6 space-y-3 sm:space-y-4">
              <div>
                <h3 class="text-base sm:text-lg font-semibold text-white mb-2 flex items-center">
                  <span class="w-2 h-2 rounded-full bg-white/40 mr-2"></span>
                  Technology Stack
                </h3>
                <p class="text-white/50 text-xs sm:text-sm font-light">AI content recommendation system, natural language processing, community analysis</p>
              </div>
              <div>
                <h3 class="text-base sm:text-lg font-semibold text-white mb-2 flex items-center">
                  <span class="w-2 h-2 rounded-full bg-white/40 mr-2"></span>
                  AI Application Highlights
                </h3>
                <p class="text-white/60 leading-relaxed text-sm sm:text-base font-light">
                  Utilizing AI algorithms for content matching and recommendations, optimizing user communication experience through natural language processing technology to build stronger community connections.
                </p>
              </div>
            </div>

            <a
              href="https://wootogether.com"
              target="_blank"
              rel="noopener noreferrer"
              class="inline-flex items-center text-base sm:text-lg md:text-xl font-normal transition-all underline underline-offset-4 decoration-yellow-200/30 hover:decoration-yellow-200/50 text-yellow-200 hover:text-yellow-100"
            >
              Learn More
              <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14" />
              </svg>
            </a>
          </div>
        </div>

        <!-- QuisPay -->
        <div class="bg-black border border-white/15 rounded-none transition-all duration-300 overflow-hidden hover:border-white/25">
          <div class="p-5 sm:p-6 md:p-8">
            <div class="flex items-center justify-between mb-4">
              <span class="px-3 py-1 rounded-none text-xs font-normal uppercase tracking-wider bg-gray-300 text-black border border-gray-400">
                Fintech AI
              </span>
            </div>
            <a
              href="https://quispay.com"
              target="_blank"
              rel="noopener noreferrer"
              class="text-2xl sm:text-3xl font-bold mb-3 sm:mb-4 block hover:opacity-80 transition-opacity text-yellow-200"
            >
              QuisPay
            </a>
            <p class="text-white/60 mb-4 sm:mb-6 leading-relaxed text-sm sm:text-base font-light">
              Innovative fintech payment platform leveraging AI technology to deliver seamless and secure payment solutions for businesses and consumers.
            </p>
            
            <div class="mb-4 sm:mb-6 space-y-3 sm:space-y-4">
              <div>
                <h3 class="text-base sm:text-lg font-semibold text-white mb-2 flex items-center">
                  <span class="w-2 h-2 rounded-full bg-white/40 mr-2"></span>
                  Technology Stack
                </h3>
                <p class="text-white/50 text-xs sm:text-sm font-light">AI-powered payment processing, intelligent fraud detection, real-time transaction analytics</p>
              </div>
              <div>
                <h3 class="text-base sm:text-lg font-semibold text-white mb-2 flex items-center">
                  <span class="w-2 h-2 rounded-full bg-white/40 mr-2"></span>
                  AI Application Highlights
                </h3>
                <p class="text-white/60 leading-relaxed text-sm sm:text-base font-light">
                  Utilizing advanced AI algorithms for real-time fraud detection and risk assessment, optimizing payment processing efficiency through intelligent routing and providing comprehensive transaction analytics to enhance payment security and user experience.
                </p>
              </div>
            </div>

            <a
              href="https://quispay.com"
              target="_blank"
              rel="noopener noreferrer"
              class="inline-flex items-center text-base sm:text-lg md:text-xl font-normal transition-all underline underline-offset-4 decoration-yellow-200/30 hover:decoration-yellow-200/50 text-yellow-200 hover:text-yellow-100"
            >
              Learn More
              <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14" />
              </svg>
            </a>
          </div>
        </div>

        <!-- AI Application Incubation -->
        <div class="bg-black border border-white/15 rounded-none transition-all duration-300 overflow-hidden hover:border-white/25">
          <div class="p-5 sm:p-6 md:p-8">
            <div class="flex items-center justify-between mb-4">
              <span class="px-3 py-1 rounded-none text-xs font-normal uppercase tracking-wider bg-gray-300 text-black border border-gray-400">
                AI Incubation
              </span>
            </div>
            <h2 class="text-2xl sm:text-3xl font-bold mb-3 sm:mb-4 text-white">AI Application Incubation</h2>
            <p class="text-white/60 mb-4 sm:mb-6 leading-relaxed text-sm sm:text-base font-light">
              Early-stage AI startup projects we are incubating, focused on solving real business problems.
            </p>
            
            <div class="mb-4 sm:mb-6 space-y-3 sm:space-y-4">
              <div>
                <h3 class="text-base sm:text-lg font-semibold text-white mb-2 flex items-center">
                  <span class="w-2 h-2 rounded-full bg-white/40 mr-2"></span>
                  Technology Stack
                </h3>
                <p class="text-white/50 text-xs sm:text-sm font-light">Machine learning, deep learning, natural language processing</p>
              </div>
              <div>
                <h3 class="text-base sm:text-lg font-semibold text-white mb-2 flex items-center">
                  <span class="w-2 h-2 rounded-full bg-white/40 mr-2"></span>
                  AI Application Highlights
                </h3>
                <p class="text-white/60 leading-relaxed text-sm sm:text-base font-light">
                  Assisting startup teams in applying AI technology to real business scenarios, from proof of concept to product-market fit, providing comprehensive technical and business support.
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>

      <!-- Stats Section -->
      <div class="mt-12 sm:mt-16 md:mt-20 bg-black border border-white/15 rounded-none p-6 sm:p-8 md:p-12">
        <h2 class="text-2xl sm:text-3xl font-bold text-white mb-6 sm:mb-8 text-center">Investment Results</h2>
        <div class="grid grid-cols-2 md:grid-cols-4 gap-4 sm:gap-6">
          <div class="text-center p-6 bg-black border border-white/15 rounded-none">
            <div class="text-4xl font-bold text-yellow-200 mb-2">4+</div>
            <div class="text-yellow-200/80 font-light">Investment Projects</div>
          </div>
          <div class="text-center p-6 bg-black border border-white/15 rounded-none">
            <div class="text-4xl font-bold text-yellow-200 mb-2">2020</div>
            <div class="text-yellow-200/80 font-light">Founded</div>
          </div>
          <div class="text-center p-6 bg-black border border-white/15 rounded-none">
            <div class="text-4xl font-bold text-yellow-200 mb-2">100%</div>
            <div class="text-yellow-200/80 font-light">AI Technology</div>
          </div>
          <div class="text-center p-6 bg-black border border-white/15 rounded-none">
            <div class="text-4xl font-bold text-yellow-200 mb-2">âˆž</div>
            <div class="text-yellow-200/80 font-light">Continuous Growth</div>
          </div>
        </div>
      </div>
    </div>
  </div>
</BaseLayout>


